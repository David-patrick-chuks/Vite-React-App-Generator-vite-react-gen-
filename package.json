{
  "name": "vite-react-generator",
  "version": "1.2.2",
  "main": "build/cli.js",
  "type": "module",
  "scripts": {
    "build": "javascript-obfuscator bin --output build --compact true --control-flow-flattening true"
  },
  "bin": {
    "vite-react-generator": "build/cli.js",
    "vrt": "build/cli.js"
  },
  "keywords": [
    "vite",
    "vite-react-gen",
    "vrt",
    "react",
    "vite-react",
    "vite generator",
    "react generator",
    "tailwindcss",
    "tailwind",
    "google fonts",
    "poppins",
    "frontend",
    "app generator",
    "vite app",
    "react vite",
    "web development",
    "modern web",
    "react starter",
    "vite starter",
    "react-vite-gen"
  ],
  "preferGlobal": true,
  "files": [
    "LICENSE",
    "README.md",
    "SCREENCAST.svg",
    "build/",
    "templates-extra/"
  ],
  "author": "David Patrick <pd3072894@gmail.com> (https://www.davidtsx.vercel.app/)",
  "license": "MIT",
  "description": "A streamlined Vite + React app generator with a minimal yet powerful setup. Includes pre-configured Tailwind CSS for styling and Google Font Poppins for modern typography, providing a fast and optimized development environment for building applications.",
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "figlet": "^1.8.0",
    "fs-extra": "^11.2.0",
    "log-symbols": "^7.0.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "javascript-obfuscator": "^4.1.1",
    "validate-npm-package-name": "^6.0.0"
  },
  "homepage": "https://github.com/David-patrick-chuks/vite-react-gen",
  "bugs": {
    "url": "https://github.com/David-patrick-chuks/vite-react-gen/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/David-patrick-chuks/vite-react-gen.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://buymeacoffee.com/davidpatrickchuks"
    }
  ]
}
